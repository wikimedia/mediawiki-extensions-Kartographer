{
	"root": true,
	"env": {
		"commonjs": true
	},
	"extends": [
		"wikimedia/client-es6",
		"wikimedia/jquery",
		"wikimedia/mediawiki",
		"wikimedia/jsdoc"
	],
	"globals": {
		"L": "readonly",
		"Kartographer": "readonly",
		"Set": "readonly"
	},
	"rules": {
		"jsdoc/check-tag-names": "error",
		"jsdoc/require-returns": "off",
		"jsdoc/require-returns-check": "off",
		"max-len": "off"
	}
}
