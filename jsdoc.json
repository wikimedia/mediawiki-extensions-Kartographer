{
	"opts": {
		"destination": "docs/js",
		"package": "package.json",
		"pedantic": true,
		"recurse": true,
		"allowedGlobals": [
			"_addItem",
			"_addScales",
			"_fixMapSize",
			"_invalidateInteractive",
			"_onInputClick",
			"_onToggleScale",
			"_toggleScale",
			"_updateScales",
			"activateRouter",
			"addDataGroups",
			"addDataLayer",
			"addGeoJSONGroups",
			"addGeoJSONLayer",
			"alertExternalData",
			"bracketDevicePixelRatio",
			"buildAttribution",
			"closeFullScreen",
			"createPopupHtml",
			"doWhenReady",
			"fetchThumbnail",
			"getHash",
			"getInitialMapPosition",
			"getKartographerLayer",
			"getLayerUrl",
			"getMapData",
			"getMapDialog",
			"getMapPosition",
			"getScaleLatLng",
			"getValidBounds",
			"getWindowManager",
			"handleMapLinks",
			"initMapBox",
			"initMapframeFromElement",
			"initView",
			"invalidateSizeAndSetInitialView",
			"isStatic",
			"maplinks",
			"maps",
			"mwApi",
			"onAdd",
			"onMapMove",
			"openFullScreen",
			"remove",
			"restoreUnparsedText",
			"routerInited",
			"setLang",
			"setView",
			"thumbnailCache",
			"toggleStaticState",
			"updateHash",
			"updateKartographerLayer",
			"validateBounds"
		],
		"template": "node_modules/jsdoc-wmf-theme"
	},
	"plugins": [
		"plugins/markdown"
	],
	"source": {
		"include": [ "modules" ]
	},
	"templates": {
		"cleverLinks": true,
		"default": {
			"useLongnameInNav": true
		},
		"wmf": {
			"repository": "https://gerrit.wikimedia.org/g/mediawiki/extensions/Kartographer/",
			"linkMap": {
				"jQuery": "https://api.jquery.com/Types/#jQuery",
				"jQuery.Promise": "https://api.jquery.com/Types/#Promise",
				"ve.ce.MWBlockExtensionNode": "https://doc.wikimedia.org/VisualEditor/master/js/#!/api/ve.ce.MWBlockExtensionNode",
				"ve.ce.MWInlineExtensionNode": "https://doc.wikimedia.org/VisualEditor/master/js/#!/api/ve.ce.MWInlineExtensionNode",
				"ve.ce.ResizableNode": "https://doc.wikimedia.org/VisualEditor/master/js/#!/api/ve.ce.ResizableNode",
				"ve.ce.ResizableNode.getAttributeChanges": "https://doc.wikimedia.org/VisualEditor/master/js/#!/api/ve.ce.ResizableNode-method-getAttributeChanges",
				"ve.ce.ResizableNode.onResizableResizing": "https://doc.wikimedia.org/VisualEditor/master/js/#!/api/ve.ce.ResizableNode-method-onResizableResizing",
				"ve.dm.MWBlockExtensionNode": "https://doc.wikimedia.org/VisualEditor/master/js/#!/api/ve.dm.MWBlockExtensionNode",
				"ve.dm.ModelNode": "https://doc.wikimedia.org/VisualEditor/master/js/#!/api/ve.dm.Model",
				"ve.dm.MWInlineExtensionNode": "https://doc.wikimedia.org/VisualEditor/master/js/#!/api/ve.dm.MWInlineExtensionNode",
